---
import { ViewTransitions } from 'astro:transitions';
export interface Props {
	title: string;
}

const { title } = Astro.props;
---

<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/svg+xml" href="/favicon.svg" />
    <title>{title}</title>
</head>
<ViewTransitions />
